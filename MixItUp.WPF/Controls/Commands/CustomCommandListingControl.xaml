<UserControl x:Class="MixItUp.WPF.Controls.Commands.CustomCommandListingControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MixItUp.WPF.Controls.Overlay"
             xmlns:CommandsControls="clr-namespace:MixItUp.WPF.Controls.Commands"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <GroupBox x:Name="GroupBox">
        <CommandsControls:CommandListingButtonsControl x:Name="CommandControl" DataContext="{Binding}" HorizontalAlignment="Center" EditClicked="CommandControl_EditClicked" HideDeleteButton="True" HideEnableDisableToggle="True" />
    </GroupBox>
</UserControl>
