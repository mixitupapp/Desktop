<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Light" Color="#DD004040" po:Freeze="True" />                   <!-- Emphasis Background -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Light.Foreground" Color="#DDFFFFFF" po:Freeze="True" />         <!-- Emphasis Text -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary" Color="#DD66023C" po:Freeze="True" />                     <!-- Main Background -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Foreground" Color="#DD00A86B" po:Freeze="True" />           <!-- Main Foreground -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Dark" Color="#DD00A86B" po:Freeze="True" />                    <!-- Highlight Background -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Dark.Foreground" Color="Black" po:Freeze="True" />              <!-- Unknown... -->

    <SolidColorBrush x:Key="MainApplicationBackground" Color="Black" po:Freeze="True" />                  <!-- Application Background -->
</ResourceDictionary>