<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"> 
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Light" Color="#DD3F2845" po:Freeze="True" />                   <!-- Emphasis Background -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Light.Foreground" Color="#DDB18CBD" po:Freeze="True" />         <!-- Emphasis Text -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary" Color="#DDB18CBD" po:Freeze="True" />                     <!-- Main Background -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Foreground" Color="#DD3F2845" po:Freeze="True" />           <!-- Main Foreground -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Dark" Color="#DD9463A5" po:Freeze="True" />                    <!-- Highlight Background -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Dark.Foreground" Color="#DD3F2845" po:Freeze="True" />          <!-- Unknown... -->

  <SolidColorBrush x:Key="MainApplicationBackground" Color="White" po:Freeze="True" />                  <!-- Application Background -->
</ResourceDictionary>