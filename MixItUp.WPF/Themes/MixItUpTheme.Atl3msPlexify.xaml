<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Light" Color="#DDCC7B19" po:Freeze="True" />                   <!-- Emphasis Background -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Light.Foreground" Color="#DDFDFDFD" po:Freeze="True" />         <!-- Emphasis Text -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary" Color="#DD1F2326" po:Freeze="True" />                     <!-- Main Background -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Foreground" Color="#DDE5A00D" po:Freeze="True" />           <!-- Main Foreground -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Dark" Color="#DD465252" po:Freeze="True" />                    <!-- Highlight Background -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Dark.Foreground" Color="Black" po:Freeze="True" />              <!-- Unknown... -->

    <SolidColorBrush x:Key="MainApplicationBackground" Color="Black" po:Freeze="True" />                  <!-- Application Background -->
</ResourceDictionary>