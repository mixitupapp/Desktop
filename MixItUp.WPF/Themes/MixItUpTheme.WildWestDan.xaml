<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:System="clr-namespace:System;assembly=mscorlib">
  <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#946f4d" po:Freeze="True" />                     <!-- Emphasis Background -->
  <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#f8ecb4" po:Freeze="True" />           <!-- Emphasis Text -->
  <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#3b2446" po:Freeze="True" />                       <!-- Main Background -->
  <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#e8c51c" po:Freeze="True" />             <!-- Main Foreground -->
  <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#753c90" po:Freeze="True" />                      <!-- Highlight Background -->
  <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="Black" po:Freeze="True" />              <!-- Unknown... -->

  <SolidColorBrush x:Key="MainApplicationBackground" Color="#DD000000" po:Freeze="True" />              <!-- Application Background -->

  <System:String x:Key="BaseTheme">Mixer</System:String>                                                <!-- Base Theme -->
</ResourceDictionary>