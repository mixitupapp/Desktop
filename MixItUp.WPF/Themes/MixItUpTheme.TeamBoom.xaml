<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"> 
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Light" Color="Black" po:Freeze="True" />                       <!-- Emphasis Background -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Light.Foreground" Color="Teal" po:Freeze="True" />              <!-- Emphasis Text -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary" Color="Purple" po:Freeze="True" />                        <!-- Main Background -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Foreground" Color="Teal" po:Freeze="True" />                <!-- Main Foreground -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Dark" Color="Teal" po:Freeze="True" />                         <!-- Highlight Background -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Dark.Foreground" Color="Teal" po:Freeze="True" />               <!-- Unknown... -->

  <SolidColorBrush x:Key="MainApplicationBackground" Color="Black" po:Freeze="True" />                  <!-- Application Background -->
</ResourceDictionary>