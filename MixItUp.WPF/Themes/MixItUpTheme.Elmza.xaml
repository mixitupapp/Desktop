<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:System="clr-namespace:System;assembly=mscorlib">
  <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#E09590" po:Freeze="True" />                     <!-- Emphasis Background -->
  <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#FFFFFF" po:Freeze="True" />           <!-- Emphasis Text -->
  <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#B2D09B" po:Freeze="True" />                       <!-- Main Background -->
  <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#FFFFDD" po:Freeze="True" />             <!-- Main Foreground -->
  <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#89AE6E" po:Freeze="True" />                      <!-- Highlight Background -->
  <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="Black" po:Freeze="True" />              <!-- Unknown... -->

  <SolidColorBrush x:Key="MainApplicationBackground" Color="White" po:Freeze="True" />              <!-- Application Background -->
</ResourceDictionary>