<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:System="clr-namespace:System;assembly=mscorlib">
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Light" Color="#AF528D" po:Freeze="True" />                     <!-- Emphasis Background -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Light.Foreground" Color="#FFFFFF" po:Freeze="True" />           <!-- Emphasis Text -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary" Color="#5B366A" po:Freeze="True" />                       <!-- Main Background -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Foreground" Color="#9A86E9" po:Freeze="True" />             <!-- Main Foreground -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Dark" Color="#AF528D" po:Freeze="True" />                      <!-- Highlight Background -->
  <SolidColorBrush x:Key="MaterialDesign.Brush.Primary.Dark.Foreground" Color="Black" po:Freeze="True" />              <!-- Unknown... -->

  <SolidColorBrush x:Key="MainApplicationBackground" Color="White" po:Freeze="True" />              <!-- Application Background -->
</ResourceDictionary>